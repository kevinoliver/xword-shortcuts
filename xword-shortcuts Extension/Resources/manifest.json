{
    "manifest_version": 3,
    "default_locale": "en",

    "name": "xword-shortcuts",
    "description": "Adds keyboard shortcuts to the NYT crossword",
    "version": "1.0",

    "icons": {
        "48": "images/icon-48.png",
        "96": "images/icon-96.png",
        "128": "images/icon-128.png",
        "256": "images/icon-256.png",
        "512": "images/icon-512.png"
    },

    "content_scripts": [{
        "js": [ "content.js" ],
        "matches": [ "*://nytimes.com/crosswords/*" ]
    }],

    "permissions": [ ]
}
